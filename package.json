    "start:watcher": "ts-node src/watcher/watcher.ts"
  },
  "dependencies": {
    "@solana/web3.js": "^1.73.0",
    "dotenv": "^16.0.0",
    "ethers": "^6.0.0",
    "mongodb": "^5.1.0",
    "nodemon": "^3.1.9",
    "puppeteer": "^24.1.1",
    "twitter-api-v2": "^1.13.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
