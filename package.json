{
  "name": "x-sniper",
  "version": "1.0.0",
  "description": "X sniper project: listens to Twitter/X and auto-buys contracts",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
